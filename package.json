{
  "name": "cpu-monitor",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "d3": "^7.0.0",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@types/d3": "^7.0.0",
    "concurrently": "^6.2.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0"
  },
  "scripts": {
    "start": "concurrently 'node ./backend' 'react-scripts start'",
    "build": "react-scripts build",
    "test:backend": "jest ./backend/*.spec.js",
    "test:ui": "react-scripts test",
    "test": "yarn test:backend && yarn test:ui",
    "eject": "react-scripts eject",
    "serve": "yarn build && node ./backend"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!d3)/"
    ]
  },
  "proxy": "http://localhost:4242"
}
